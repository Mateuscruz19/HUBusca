{
  "expo": {
    "name": "HUBusca",
    "slug": "HUBusca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/HUBusca-favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/HUBusca-favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.mlc.hubusca",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.mlc.hubusca",
      "versionCode": 1,
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "8f7b7008-71d4-4298-8f15-391c881fcf79"
      }
    }
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/HUBusca-favicon.png"
  },
  "plugins": [
    "expo-router"
  ],
  "experiments": {
    "typedRoutes": true
  }
}
